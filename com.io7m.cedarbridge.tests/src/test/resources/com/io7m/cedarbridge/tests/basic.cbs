(package com.io7m.cedarbridge)

(import x.y.z b)

(variant UnitType
  [record U])

(variant Option
  [parameter T]
  [record None]
  [record Some (field value T)])

(variant List
  [parameter T]
  [record Null]
  [record Cons
    (field head T)
    (field rest [List T])])

(record Pair
  [parameter A]
  [parameter B]
  [field f0 A]
  [field f1 B])

(record Map
  [parameter K]
  [parameter V]
  [field entries (List [Pair K V])])
