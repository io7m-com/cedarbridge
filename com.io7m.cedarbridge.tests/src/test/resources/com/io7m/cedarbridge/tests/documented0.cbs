(package com.io7m.cedarbridge)

(import x.y.z b)

(variant UnitType)

(variant Option
  [parameter T]
  (documentation T "A T")
  [case None]
  (documentation None "A None")
  [case Some
    (field value T)
    (documentation value "A value")]
  (documentation Some "A Some"))

(documentation Option "An Option")

(record Pair
  [parameter A]
  (documentation A "A A")
  [parameter B]
  (documentation B "A B")
  [field f0 A]
  (documentation f0 "A f0")
  [field f1 B]
  (documentation f1 "A f1"))

(documentation Pair "A Pair")

(protocol Z
  [version 0
    UnitType])

(documentation Z "A Z")
